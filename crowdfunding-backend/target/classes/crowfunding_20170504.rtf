CREATE TABLE `Rate` (\
  `pid` INT NOT NULL,\
  `uemail` varchar(40) NOT NULL,\
  `rate` INT NOT NULL,\
  PRIMARY KEY (`pid`, `uemail`),\
  KEY `pid` (`pid`),\
  CONSTRAINT `rate_check_success` CHECK (0 < (SELECT COUNT(*) FROM Request r WHERE pid = r.pid and 'success')),\
