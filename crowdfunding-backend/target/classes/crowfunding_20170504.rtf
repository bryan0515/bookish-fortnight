-- View structure for `FundingRequest`\
-- ----------------------------\
DROP VIEW IF EXISTS `FundingRequest`;\
CREATE VIEW FundingRequest AS\
SELECT	*\
FROM	Request\
NATURAL JOIN (\
	SELECT	 pid, sttime, sum(amount) as curFund\
	FROM	`Pledge` \
	GROUP BY pid, sttime\
) temp\
WHERE status = 'funding';}